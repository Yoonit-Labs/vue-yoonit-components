{
  "name": "yoonit-components",
  "version": "0.1.0",
  "private": true,
  "main": "build/yoonit-components.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib & npm run build:wc",
    "build:lib": "vue-cli-service build --target lib --name yoonit-components src/main.js",
    "build:wc": "vue-cli-service build --target wc-async --name yoonit-components 'src/*.vue'",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chromedriver": "87",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "geckodriver": "^1.20.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "svg-url-loader": "^7.1.1",
    "url-loader": "^4.1.1",
    "vue-template-compiler": "^2.6.11"
  }
